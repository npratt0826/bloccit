 <h1>All Questions</h1>
 
 <div class="row-md">
 <%= link_to "New Question", new_question_path, class: 'btn btn-success' %>
 </div>
 <% @questions.each do |question| %>
   <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <%= link_to "#{question.title}: #{question.resolved}", question %>
       </h4>
     </div>
   </div>
   
 <% end %>
 