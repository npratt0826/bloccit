 <div class="jumbotron">
   <h1>Sign Up Confirmation</h1>
   <h3>Are you sure these values are correct</h3>
   <p>
       <ul>
           <li>Email: <%= @user.email %></li>
           <li>Name: <%= @user.name %> </li>
       </ul>
   </p>
   </p>
   <div>
       <%= button_to "Yes", {controller: "users", action: "create", params: params}, class: 'btn btn-success' %>
       <%= button_to "No", {controller: "users", action: "create", params: params}, class: 'btn btn-danger' %>
   </div>
 </div>
 